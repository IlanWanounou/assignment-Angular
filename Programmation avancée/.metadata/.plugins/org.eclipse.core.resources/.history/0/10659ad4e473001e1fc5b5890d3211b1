package tp1;

public class ModuleEnseignement {
	private String NomModule;
	private static int AnneeCreation;
	private String NomDiplome;
	private static String NomEnseignant;
	private static Type type;
	private static Boolean rattrapage;
	
	
	public ModuleEnseignement( String NomModule, int AnneeCreation, String NomDiplome,
	String NomEnseignant,
	Type type,
	Boolean rattrapage) {
		this.NomModule = NomModule;
		this.AnneeCreation = AnneeCreation;
		this.NomDiplome = NomDiplome;
		this.NomEnseignant = NomEnseignant;
		this.type = type;
		this.rattrapage = rattrapage;
	}
	
	public int getAnneeCreation() {
		return AnneeCreation;
	}

	public void setAnneeCreation(int anneeCreation) {
		AnneeCreation = anneeCreation;
	}
	
	 static class staticClass {
	      public void incremanteYear() {
	    	  AnneeCreation++;
	      }
	      
	      public void incremanteCreation() {
	    	  if(NomEnseignant == "Dupont") AnneeCreation++;
	      }
	      
	      public void removeRattrapage() {
	    	  if(type == Type.QCM) rattrapage = false;
	      }
	 }
	
}
