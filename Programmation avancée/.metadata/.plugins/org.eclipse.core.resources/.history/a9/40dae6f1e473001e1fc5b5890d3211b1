package tp1;

import java.util.ArrayList;

public class main {

	public static void main(String[] args) {
		ModuleEnseignement ME1 = new ModuleEnseignement("ME1", 2020, "L3", "Dupont", Type.Projet, true);
		ModuleEnseignement ME2 = new ModuleEnseignement("ME2", 2021, "M1", "Antoine", Type.QCM, true);
		ModuleEnseignement ME3 = new ModuleEnseignement("ME3", 2022, "M2", "Dupont", Type.Question_synth√®se, true);
		
		ArrayList<ModuleEnseignement> list = new ArrayList<ModuleEnseignement>();
		list.add(ME1);
		list.add(ME2);
		list.add(ME3);
		
		for (ModuleEnseignement m : list) {
			ModuleEnseignement.staticClass test = new ModuleEnseignement.staticClass();
			
			test.incremanteYear(m.getAnneeCreation());
			System.out.print(m.getAnneeCreation());
			
			
		}
	}

}
